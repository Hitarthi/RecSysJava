<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
 <class name="GetterSetter.GS_Reg" table="user">  
 <id name="id">  
 <generator class="increment"></generator>  
 </id> 
 <property name="userid"></property> 
 <property name="fname"></property>  
 <property name="lname"></property>  
 <property name="email"></property>  
 <property name="pswd"></property>
 <property name="date"></property>  
 <property name="num"></property>  
 <property name="t"></property>

 </class>  
  </hibernate-mapping>
